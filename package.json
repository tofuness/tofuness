{
  "name": "tofuness",
  "version": "1.0.0",
  "main": "server.js",
  "repository": {},
  "author": "Dennis Jin <dennis@tofuness.com>",
  "license": "UNLICENSED",
  "scripts": {
    "install": "yarn run webpack:build",
    "webpack:dev": "better-npm-run webpack:dev",
    "webpack:build": "better-npm-run webpack:build"
  },
  "betterScripts": {
    "webpack:dev": {
      "command": "webpack-dev-server --hot --inline --config webpack.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "webpack:build": {
      "command": "webpack -p --config webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.0",
    "babel-preset-es2015": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "express": "^4.14.1",
    "hbs": "^4.0.1"
  }
}
